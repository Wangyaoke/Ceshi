<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >
    <RelativeLayout
        android:id="@+id/relative_out"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >
        <RelativeLayout
            android:id="@+id/relative_view"
            android:layout_width="270dp"
            android:layout_height="143dp"
            android:layout_marginLeft="43dp"
            android:layout_marginRight="43dp"
            android:layout_marginTop="215dp"
            android:layout_centerHorizontal="true"
            android:background="@mipmap/popup_bg3x"
            >
            <ImageView
                android:id="@+id/obtain_yanZheng_tuxing"
                android:layout_width="54dp"
                android:layout_height="67dp"

                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true"

                android:clickable="true"



                />
        </RelativeLayout>
        <LinearLayout
            android:id="@+id/relative_in"
            android:layout_width="270dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="43dp"
            android:layout_marginRight="43dp"
            android:layout_centerInParent="true"
            android:layout_below="@+id/relative_view"

            android:orientation="vertical"
            android:background="@drawable/shape_popwindow_bottom"
            >

            <TextView
                android:background="@color/white"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="确定清除浏览历史？"
                android:textColor="@color/gray_dan"
                android:textSize="14sp"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="15dp"
                android:layout_gravity="center"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/subscribe_item_disabled_bg_day"
                android:layout_marginTop="21dp"
                />
        <LinearLayout

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/relative_view"

            android:orientation="horizontal"
            >
            <TextView
                android:id="@+id/tv_cancel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="取消"
                android:gravity="center"
                android:textColor="@color/text_blue"
                android:layout_weight="1"
                android:layout_marginBottom="16dp"
                android:layout_marginTop="16dp"
                android:textSize="16sp"
                android:clickable="true"

                />
            <TextView
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/subscribe_item_disabled_bg_day"

                />
            <TextView
                android:id="@+id/tv_sure"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="确定"
                android:gravity="center"
                android:layout_weight="1"
                android:textColor="@color/text_blue"
                 android:layout_marginBottom="16dp"
                android:layout_marginTop="16dp"
                android:textSize="16sp"
                android:clickable="true"

                />

        </LinearLayout>
        </LinearLayout>

    </RelativeLayout>

</RelativeLayout>